<%= simple_form_for(member) do |m| %>
<%= m.input :name, label: "Name" %>
<%= m.input :phone, label: "Phone" %>
<%= m.input :email, label: "E-mail" %>
<%= m.submit :class => 'btn btn-outline-primary', :style => 'display: inline-block' %>
<%= link_to 'Back to Members List', members_path, :class => 'btn btn-outline-secondary', :style => 'display: inline-block' %>
<% end %>